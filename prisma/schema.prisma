generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("DATABASE_URL")
}

model Link {
  id          String   @id @default(uuid())
  code        String   @unique
  targetUrl   String
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
  lastClicked DateTime?
}
